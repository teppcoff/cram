<%= render "search_form" %>
<table class="table">
    <tr>
        <th scope="col">名称</th>
        <th scope="col">詳細</th>
    </tr>
    <%= @goal_sheets.each do |sheet| %>
    <tr>
        <td><%= sheet.name %></td>
        <td><%= button_to "表示", staff_goal_sheet_path(sheet.id), method: "get", class: "btn btn-outline-info" %></td>
    </tr>
    <% end %>
</table>
<%= paginate @goal_sheets %>