<%= form_with model: @event, url: staff_events_edit_path, local: true do |f| %>
    <%= render "form", f: f %>
    <div class="btn-group">
        <%= f.submit "更新", class: "btn btn-outline-success" %>
    </div>
<% end %> 
<%= button_to "戻る", staff_event_path(@event.id), method: :get, class: "btn btn-outline-secondary" %>