<div class="title border-bottom mt-4 mb-4">
    <p class="h5">定期テスト結果登録</p>
</div>
<%= form_with model: @score_sheet, url: student_score_sheets_path, local: true do |f| %>
    <%= render "layouts/error_messages", model: f.object %>
    <%= render "form", f: f %>
    <div class="form-submit">
        <%= f.submit "登録", class: "btn btn-outline-primary" %>
    </div>
<% end %>