<table class="table">
    <th scope="col">名称</th>
    <% @goal_sheets.each do |sheet| %>
        <tr>
        <td><%= sheet.name %></td>
        <td><%=  button_to "表示", student_goal_sheet_path(sheet.id), method: "get", class: "btn btn-outline-info" %></td>
        </tr>
    <% end %>
</table>
<%= button_to "トップに戻る", student_root_path, method: "get", class: "btn btn-outline-secondary" %>
<%= paginate @goal_sheets %>